<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 填充 -->
    <solid android:color="#0086d0" /> <!-- 定义填充的颜色值 -->


    <!-- 描边 -->
    <stroke
        android:width="2dp"
        android:color="#0086d0" /> <!-- 定义描边的宽度和描边的颜色值 -->


    <!-- 圆角 -->
    <corners
        android:bottomLeftRadius="0dp"
        android:topLeftRadius="0dp" /> <!-- 设置四个角的半径 -->

</shape>